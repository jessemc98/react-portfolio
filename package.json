{
  "name": "Simple-React-Portfolio",
  "version": "1.0.0",
  "description": "personal portfolio project built with react",
  "main": "src/main.js",
  "scripts": {
    "start": "npm-run-all --parallel serve test:watch",
    "build": "webpack && npm run serve:build",
    "serve:build": "webpack --watch",
    "serve": "webpack-dev-server",
    "test:lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --cache",
    "test": "mocha --color --reporter spec libs/test.js src/*.test.js src/**/*.test.js src/components/**/*.test.js src/components/**/**/*.test.js",
    "test:watch": "npm test -- --watch"
  },
  "author": "Jesse McIntosh",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "6.5.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.16.3",
    "clean-webpack-plugin": "0.1.14",
    "css-loader": "0.26.1",
    "enzyme": "2.4.1",
    "eslint": "3.7.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.0.0",
    "eslint-plugin-react": "6.3.0",
    "expect": "1.20.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "0.9.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "2.26.0",
    "jsdom": "9.6.0",
    "mocha": "3.1.0",
    "node-sass": "3.10.1",
    "npm-run-all": "3.1.0",
    "path": "0.12.7",
    "postcss-loader": "1.2.1",
    "react-addons-test-utils": "15.3.2",
    "react-lite": "0.15.6",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "url": "0.11.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.28",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "2.3.1"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "focus-trap-react": "3.0.1",
    "react": "15.3.2",
